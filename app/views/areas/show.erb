<div class="page">
<h3 class="region"> Region: <%= @area.name %></h3>
<h4 class="zip_code"> Zip Code: <%= @area.zip_code %></h4>
 <p> Categories: <% @area.listing_categories.each do |listing_category| %>
    <li class="categories">
    <%= link_to listing_category.name, listing_category %> <p class= "small"> Listings: <%= listing_category.listing_amount %>
    </li>
<%end%>
</div>

